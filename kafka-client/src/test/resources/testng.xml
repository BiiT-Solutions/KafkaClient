<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="D3-clientTests">
    <test name="D3-clientTests" enabled="true" preserve-order="true">
        <groups>
            <run>
                <!-- Kafka server is down on testing -->
                <exclude name="kafkaTemplateEvents"/>
                <!-- Disabled as not used -->
                <exclude name="ksqlClient"/>
            </run>
        </groups>
        <classes>
            <class name="com.biit.kafka.tests.KafkaTemplateTests"/>
            <class name="com.biit.kafka.tests.KsqlClientTest"/>
        </classes>
    </test>
</suite>
