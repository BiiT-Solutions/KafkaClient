    <logger name="com.biit.usmo.logger.DroolsClientLogger" additivity="false" level="DEBUG">
        <appender-ref ref="DAILY"/>
        <appender-ref ref="CONSOLE"/>
    </logger>
    <logger name="com.biit.usmo.logger.TasksLogger" additivity="false" level="INFO">
