        <appender-ref ref="DAILY"/>
    </logger>
    <logger name="org.hibernate.tool.hbm2ddl" additivity="false" level="WARN">
        <appender-ref ref="CONSOLE"/>
        <appender-ref ref="DAILY"/>
    </logger>
    <logger name="org.springframework" additivity="false" level="ERROR">
        <appender-ref ref="CONSOLE"/>
        <appender-ref ref="DAILY"/>
    </logger>
    <logger name="com.mchange.v2.c3p0" level="OFF">
        <appender-ref ref="CONSOLE"/>
        <appender-ref ref="DAILY"/>
    </logger>
    <logger name="net.sf.ehcache.config" additivity="false" level="ERROR">
        <appender-ref ref="CONSOLE"/>
        <appender-ref ref="DAILY"/>
    </logger>
    <logger name="com.biit.usmo.logger.QrGeneratorLogger" additivity="false" level="DEBUG">
